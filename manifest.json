{
  "manifest_version": 2,
  "name": "Jira Swimlane Shuffler",
  "description": "Shuffle the order of assignee swimlanes on an active Jira sprint/Kanban board.",
  "version": "1.0",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Jira Swimlane Shuffler",
    "default_icon": "icon_32.png"
  },
  "permissions": [
    "activeTab"
  ]
}